{
  "api.version":"v1",
  "sources":[{
    "name":"<Name> Windows Metrics",
    "category":"<Name>/<env>/Windows/Metrics",
    "automaticDateParsing":false,
    "multilineProcessingEnabled":false,
    "useAutolineMatching":false,
    "contentType":"HostMetrics",
    "forceTimeZone":false,
    "filters":[],
